static void customPrint(ArrayList<String> al){
  int i,j,k,l;
  Sop(" ------------------------ ");
  Sop("| Id  | Name    | Address|");
  Sop("| __  | ____    | _______|");
  Sop("|     |         |        |")
  for(i=0;i<al.size();i++){
    Sop(al.get(i));
  }
  Sop("--------------------------");
}
